 <% if flash[:alert] %>
  <%= js add_gritter(flash[:alert], title: "Ops App Alert", sticky: false) %>
  <% elsif flash[:error] %>
  <%= js add_gritter(flash[:error], title: "Ops App Error", sticky: false) %>
  <% else %>
  <%= js add_gritter(flash[:notice], title: "Ops App Notification", sticky: false) %>
<% end %>